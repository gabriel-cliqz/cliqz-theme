    </em:targetApplication>
   
    <!-- Front End MetaData -->
    <em:name>Cliqz Theme</em:name>
    <em:internalName>cliqzTheme</em:internalName>
    <em:description>This is the default theme of Cliqz Browser</em:description>
    <em:creator>Cliqz</em:creator>
